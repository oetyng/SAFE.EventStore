@page "{databaseId}/{streamKey}/{eventId}"
@model EventModel
@{
    ViewData["Title"] = "Event details";
}
<h2>@ViewData["Title"]</h2>
<h3>@Model.Message</h3>

<div class="row">
    <div class="col-md-4">
        <div class="form-group">
            <label asp-for="@Model.Event.SequenceNr" class="control-label"></label>
            <br />
            <span>@Model.Event.SequenceNr</span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Event.Name" class="control-label"></label>
            <br />
            <span>@Model.Event.Name</span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Event.Json" class="control-label"></label>
            <br />
            <span>@Model.Event.Json</span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Event.CausationId" class="control-label"></label>
            <br />
            <span>@Model.Event.CausationId</span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Event.CorrelationId" class="control-label"></label>
            <br />
            <span>@Model.Event.CorrelationId</span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Event.TimeStamp" class="control-label"></label>
            <br />
            <span>@Model.Event.TimeStamp</span>
        </div>
    </div>
</div>